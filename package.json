{
  "name": "financeapp",
  "version": "1.0.0",
  "type": "module",
  "description": "Privacy-first finance tracker - Phase I Personal Edition",
  "scripts": {
    "test": "node test/integration.js",
    "test:backend": "cd backend && npm test",
    "dev:backend": "cd backend && npm run start:dev",
    "build:backend": "cd backend && npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:backend": "cd backend && npm run lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:backend": "cd backend && npm run format",
    "prepare": "husky",
    "install:backend": "cd backend && npm install",
    "setup": "npm install && npm run install:backend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darrenhum/FinanceApp.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/darrenhum/FinanceApp/issues"
  },
  "homepage": "https://github.com/darrenhum/FinanceApp#readme",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2"
  }
}
